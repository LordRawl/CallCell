mixin social()
  +b.social&attributes(attributes)
    block
    
mixin ui-kit-social()
  +ui-section
    +ui-section-head
      +ui-title Компонент social
      +ui-path
        p Компонент: components/social/social
      +ui-description
        p Описание компонента
  
    +ui-section-content()
      +social
      
    //- При +ui-section-content(true) контейнер отсутствует
    //- Для перечисления инлайновых блоков используется миксин +ui-elements-list()
    //- Рамка вокруг особой секции: +ui-special()